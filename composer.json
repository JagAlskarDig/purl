{
    "name": "pengzhile/purl",
    "description": "A asynchronous http client without php-curl extension.",
    "keywords": ["http", "client", "curl", "asynchronous"],
    "type": "library",
    "homepage": "http://github.com/pengzhile/purl",
    "license": "GPL2",
    "authors": [
        {
            "name": "pengzhile",
            "email": "pengzhile@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {"Purl\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Purl\\Tests\\": "tests/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.2"
        }
    }
}
